Private Sub GraphData_Click()
    Graph
End Sub

Sub Graph()

Dim xaxis As Range
Dim yaxis As Range
Set xaxis = Range("$B$4", Range("$B$4").End(xlDown))
Set yaxis = Range("$A$4", Range("$A$4").End(xlDown))

Dim f As Chart
Set f = ActiveWorkbook.Charts.Add
Set f = f.Location(Where:=xlLocationAsObject, Name:="Sheet2")
With f
    .ChartType = xlXYScatterLines
    ' set other chart properties
End With

Dim v As Series
Set v = f.SeriesCollection.NewSeries
With v
    .Values = yaxis
    .XValues = xaxis
    ' set other series properties
End With

With f.Axes(xlCategory)
    .MajorUnit = 0.0001
End With

With f
    .Axes(xlCategory, xlPrimary).HasTitle = True
    .Axes(xlCategory, xlPrimary).AxisTitle.Characters.Text = "Time"
    .Axes(xlValue, xlPrimary).HasTitle = True
    .Axes(xlValue, xlPrimary).AxisTitle.Characters.Text = "Load"
    .HasLegend = False
End With
End Sub
